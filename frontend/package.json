{
  "name": "@callput/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prod": "vite --mode prod",
    "dev": "vite --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "build:dev": "tsc && vite build --mode dev",
    "build": "npm run build:dev",
    "clean": "rm -rf dist",
    "deploy:prod": "npm run build:prod && node deploy.js --mode prod",
    "deploy:dev": "npm run build:dev && node deploy.js --mode dev",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typechain:build": "yarn run typechain --target ethers-v6 --out-dir src/typechain src/abis/*.json src/abis/**/*.json"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.435.0",
    "@callput/shared": "*",
    "@pixi/filter-drop-shadow": "^5.2.0",
    "@pixi/graphics-smooth": "^1.1.0",
    "@rainbow-me/rainbowkit": "^2.0.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^5.63.0",
    "@tippyjs/react": "^4.2.6",
    "@typechain/ethers-v6": "^0.4.3",
    "@types/pako": "^2.0.3",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "ethers": "^6.6.0",
    "html-to-image": "^1.11.11",
    "i18next": "^23.2.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "int-encoder": "^1.1.1",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "observable-hooks": "^4.2.4",
    "pako": "^2.1.0",
    "pixi.js": "^7.2.4",
    "popper.js": "^1.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.13.0",
    "react-use": "^17.5.0",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^1.13.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "viem": "^2.9.15",
    "vite-plugin-svgr": "^4.2.0",
    "wagmi": "^2.5.19"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.425.0",
